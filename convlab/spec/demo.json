  "onenet_rule_dqn_template": {
    "agent": [{
      "name": "DialogAgent",
      "nlu": {
        "name": "OneNetLU",
        "model_file": "https://convlab.blob.core.windows.net/models/onenet.tar.gz"
      },
      "dst": {
        "name": "RuleDST"
      },
      "nlg": {
        "name": "MultiwozTemplateNLG",
        "is_user": false
      },
